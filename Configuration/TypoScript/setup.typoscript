<meta name="msapplication-TileColor" content="{$gilbertsoft.gsfavicon.msapplication-TileColor}">
<meta name="msapplication-TileImage" content="/{$gilbertsoft.gsfavicon.path}ms-icon-144x144.png">
<meta name="theme-color" content="{$gilbertsoft.gsfavicon.theme-color}">
    )
  }
}

#popup.shortcutIcon < page.shortcutIcon
#popup.headerData.90 < page.headerData.90
