    90 = TEXT
    90.value (
<link rel="apple-touch-icon" sizes="57x57" href="/{$gilbertsoft.gsfavicon.path}apple-icon-57x57.png">
<link rel="apple-touch-icon" sizes="60x60" href="/{$gilbertsoft.gsfavicon.path}apple-icon-60x60.png">
<link rel="apple-touch-icon" sizes="72x72" href="/{$gilbertsoft.gsfavicon.path}apple-icon-72x72.png">
<link rel="apple-touch-icon" sizes="76x76" href="/{$gilbertsoft.gsfavicon.path}apple-icon-76x76.png">
<link rel="apple-touch-icon" sizes="114x114" href="/{$gilbertsoft.gsfavicon.path}apple-icon-114x114.png">
<link rel="apple-touch-icon" sizes="120x120" href="/{$gilbertsoft.gsfavicon.path}apple-icon-120x120.png">
<link rel="apple-touch-icon" sizes="144x144" href="/{$gilbertsoft.gsfavicon.path}apple-icon-144x144.png">
<link rel="apple-touch-icon" sizes="152x152" href="/{$gilbertsoft.gsfavicon.path}apple-icon-152x152.png">
<link rel="apple-touch-icon" sizes="180x180" href="/{$gilbertsoft.gsfavicon.path}apple-icon-180x180.png">
